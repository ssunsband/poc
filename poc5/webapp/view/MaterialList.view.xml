<mvc:View
    controllerName="poc5.controller.MaterialList"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
                        <Text text="poc5" />
                        <Button text="get" press=".onGetTest" />
                        <Text text="a" />
                        <ComboBox
                                items="{mmODataModel>/Plants}"
                                change="onChangeViType">
                                <core:ListItem key="{mmODataModel>name1}" text="{mmODataModel>name1}"/>
                            </ComboBox>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
